{
    "author": "at.eight",
    "name": "Prevent Duplicate Tabs v3",
    "description": "Prevent duplicate tabs",
    "version": "1.0",
    "manifest_version": 3,
    "default_locale": "en",
    "icons": {
        "16": "images/icon.png",
        "32": "images/32x.png",
        "48": "images/48x.png",
        "128": "images/128x.png"
    },
    "action": {
        "default_icon": "images/icon.png",
        "default_popup": "views/popup.html#popup"
    },
    "options_ui": {
        "page": "views/popup.html#config"
    },
    "background": {
        "service_worker": "js/background.js"
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    "host_permissions": [
        "*://*/*"
    ]
}